{
  "name": "jenkinscypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "aftertest": "node html-report.js",
    "removeFile": "rm -R -f .node_modules\\cypress-cucumber-preprocessor",
    "addFile":"mv -f .\\docker_support_files\\cypress-cucumber-preprocessor .\\node_modules\\cypress-cucumber-preprocessor",
    "scripts": "cypress run --headless --browser chrome --spec **/*.{feature,features}",
    "test": "npm run scripts || npm run aftertest",
    "setup": "removeFile addFile --continue-on-error"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.2.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "npm-run-all": "^4.1.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/reports/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
